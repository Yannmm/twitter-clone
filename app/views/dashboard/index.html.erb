<%= link_to 'Sign out', destroy_user_session_path, class: "btn btn-warning", data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } %>
<h1>You are logged in!</h1>
<%= form_with model: Tweet.new, url: tweets_path, local: true do |f| %>
  <%= f.text_field :body %>
  <%= f.submit %>
<% end %>
<ul>
  <% Tweet.find_each do |tweet| %>
    <li><%= tweet.body %></li>
  <% end %>
</ul>